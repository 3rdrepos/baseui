		//------------------------------------		// constructor		//------------------------------------				public function PropertiesUI(x:Number = NaN, y:Number = NaN, width:Number = NaN, height:Number = NaN) {			this.x = x;			this.y = y;			this.width = width;			this.height = height;		}				//		// PRIVATE, PROTECTED		//________________________________________________________________________________________________								// PUBLIC		//________________________________________________________________________________________________				public function toString():String {			return "[PropertiesUI] "				 + "x: " + x				 + ", y: " + y				 + ", width: " + width				 + ", height: " + height;		}			}}